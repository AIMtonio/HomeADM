	<!-- ================================ Alta de Aportaciones ======================================= -->
	<!-- Bean -->
	<dwr:configuration>
		<dwr:convert type="bean" class="aportaciones.bean.AportacionesBean" />
	</dwr:configuration>

	<!-- DAO -->
	<bean id="aportacionesDAOImpl" class="aportaciones.dao.AportacionesDAO">
		<property name="dataSource" ref="dataSource" />
		<property name="conexionOrigenDatosBean" ref="conexionOrigenDatosBeanImpl" />
		<property name="parametrosAuditoriaBean" ref="parametrosAuditoriaBeanImpl" />
		<property name="transaccionDAO" ref="transaccionDAOImpl" />
		<property name="transactionTemplate">
			<bean class="org.springframework.transaction.support.TransactionTemplate">
				<property name="transactionManager" ref="transactionManager" />
			</bean>
		</property>
		<property name="polizaDAO" ref="polizaDAOImpl" />
	</bean>

	<!-- Servicio -->
	<bean id="aportacionesServicioImpl" class="aportaciones.servicio.AportacionesServicio">
		<dwr:remote javascript="aportacionesServicio">
			<dwr:include method="consulta" />
			<dwr:include method="lista" />
			<dwr:include method="consultaRecibo" />
		</dwr:remote>
		<property name="aportacionesDAO" ref="aportacionesDAOImpl" />
		<property name="aportDispersionesDAO" ref="aportDispersionesDAOImpl" />
		<property name="monedasServicio" ref="monedasServicioImpl" />
		<property name="parametrosAuditoriaBean" ref="parametrosAuditoriaBeanImpl" />
	</bean>

	<!-- Controlador de pantalla -->
	<bean name="/altaAportacionVista.htm" class="aportaciones.controlador.AportacionesControlador">
		<property name="formView" value="aportaciones/altaAportCatalogoVista" />
		<property name="successView" value="resultadoTransaccionVista" />
		<property name="aportacionesServicio" ref="aportacionesServicioImpl" />
	</bean>

	<!-- Controlador para las Listas de Aportaciones -->
	<bean name="/listaAportaciones.htm" class="aportaciones.controlador.AportacionesListaControlador">
		<property name="aportacionesServicio" ref="aportacionesServicioImpl" />
	</bean>

