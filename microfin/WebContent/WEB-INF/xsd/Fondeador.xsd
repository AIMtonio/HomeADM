        <xs:complexType>
            <xs:sequence>
                <xs:element name="creditoID" type="xs:string" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
  
    <xs:element name="consultaCreditoFondeoBEResponse">
        <xs:complexType>
            <xs:sequence>
            	<xs:element name="creditoFondeoID"	type="xs:string" />   
                <xs:element name="tasa" 			type="xs:string" />      
                <xs:element name="moneda" 			type="xs:string" />     
