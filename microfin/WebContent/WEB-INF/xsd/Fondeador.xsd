                <xs:element name="saldoInteresAtra" type="xs:string" />     
                <xs:element name="saldoIVAInteres" 	type="xs:string" />     
                <xs:element name="saldoMoratorios" 	type="xs:string" />     
                <xs:element name="saldoIVAMora" 	type="xs:string" />     
                <xs:element name="saldoComFaltaPa"	type="xs:string" />     
                <xs:element name="saldoOtrasComis" 	type="xs:string" />     
                <xs:element name="saldoIVAComisi" 	type="xs:string" />     
                <xs:element name="saldoRetencion" 	type="xs:string" />     
                <xs:element name="saldoIVAComFalP" 	type="xs:string" />                                                                                  						 
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
     <xs:element name="consultaAmortiFondeoBERequest">    					 
        <xs:complexType>
            <xs:sequence>
                <xs:element name="creditoFondeoID" type="xs:string" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
  
    <xs:element name="consultaAmortiFondeoBEResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="ListaAmortizaciones" type="xs:string" />            						 
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>