<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
    elementFormDefault="qualified"
    targetNamespace="http://safisrv/ws/schemas"
    xmlns:schemas="http://safisrv/ws/schemas">
    
    
     <xs:element name="listaCreditoFondeoBERequest">    					 
        <xs:complexType>
            <xs:sequence>
                <xs:element name="clienteID" type="xs:string" />
            </xs:sequence>
        </xs:complexType>
    </xs:element>
  
    <xs:element name="listaCreditoFondeoBEResponse">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="listaCreditos" type="xs:string" />            						 
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    
