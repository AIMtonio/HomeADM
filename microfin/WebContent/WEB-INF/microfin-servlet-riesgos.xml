		<property name="formView" value="riesgos/parametrosRiesgosVista" /> 
	    <property name="successView" value="resultadoTransaccionVista" />
	 	<property name="parametrosRiesgosServicio" ref="parametrosRiesgosServicioImpl" /> 
	</bean>
	
	<bean name="/parametrosRiesgoGridVista.htm" class="riesgos.controlador.ParametrosRiesgosGridControlador">
