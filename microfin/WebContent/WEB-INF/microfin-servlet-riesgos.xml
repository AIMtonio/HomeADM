			</bean>
		</property>		
	</bean>
	
     	<!-- Definicion de Servicio de Parametros de Riesgos -->
	<bean id="parametrosRiesgosServicioImpl" class="riesgos.servicio.ParametrosRiesgosServicio">
		<dwr:remote javascript="parametrosRiesgosServicio">
		       <dwr:include method="consulta" />
		       <dwr:include method="listaCombo" />
		 </dwr:remote>
		 <property name="parametrosRiesgosDAO" ref="parametrosRiesgosDAOImpl" /> 
